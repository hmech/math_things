   "source": [
    "def tribonacci(signature, n):\n",
    "    sig = []\n",
    "    for i in range(n):\n",
    "        if i < 3:\n",
    "            sig.append(signature[i])\n",
    "        else:\n",
    "            sig.append(sig[i-1]+sig[i-2]+sig[i-3])\n",
    "    return sig\n",
    "\n",
    "tribonacci([1, 1, 1], 10) #[1, 1, 1, 3, 5, 9, 17, 31, 57, 105]"
   ]
